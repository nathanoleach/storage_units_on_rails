<p id="notice"><%= notice %></p>

<h1>Listing Renters</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Company</th>
      <th>Address</th>
      <th>City</th>
      <th>State</th>
      <th>Zip</th>
      <th>Phone</th>
      <th>Fax</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @renters.each do |renter| %>
      <tr>
        <td><%= renter.first_name %></td>
        <td><%= renter.last_name %></td>
        <td><%= renter.company %></td>
        <td><%= renter.address %></td>
        <td><%= renter.city %></td>
        <td><%= renter.state %></td>
        <td><%= renter.zip %></td>
        <td><%= renter.phone %></td>
        <td><%= renter.fax %></td>
        <td><%= renter.email %></td>
        <td><%= link_to 'Show', renter %></td>
        <td><%= link_to 'Edit', edit_renter_path(renter) %></td>
        <td><%= link_to 'Destroy', renter, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Renter', new_renter_path %>
